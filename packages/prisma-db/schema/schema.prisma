generator client {
  provider   = "prisma-client-js"
  output     = "../generated/client"
  engineType = "client"
}

datasource db {
  provider = "postgresql"
}

model Link {
  id           String    @id @default(cuid())
  path         String    @unique
  originalURL  String    @default("")
  qr           String    @default("")
  created      DateTime?
  modified     DateTime?
  views        Int       @default(0)
  domain       String    @default("")
  title        String?
  tags         String[]
  expiresAt    DateTime?
  expiresURL   String?
  utmSource    String?
  utmMedium    String?
  utmCampaign  String?
  utmTerm      String?
  utmContent   String?
  redirectType Int       @default(302)

  @@map("links")
}
