{
  "name": "utils",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "imports": {
    "#*": "./src/*"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch --preserveWatchOutput",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "check-types": "tsc --noEmit",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "jsonld": "^8.3.3"
  },
  "devDependencies": {
    "eslint-config": "*",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.9.2",
    "typescript-config": "*"
  },
  "publishConfig": {
    "access": "public"
  }
}