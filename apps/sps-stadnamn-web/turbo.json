{
  "$schema": "https://turborepo.org/schema.json",
  "extends": [
    "//"
  ],
  "tasks": {
    "build": {
      "outputs": [
        "dist/**",
        ".next/**"
      ],
      "dependsOn": [
        "^build"
      ],
      "env": [
        "NODE_ENV",
        "SN_ENV",
        "ES_ENDPOINT",
        "ES_TOKEN",
        "ES_ENDPOINT_TEST",
        "ES_TOKEN_TEST",
        "VERCEL_GIT_COMMIT_SHA"
      ]
    }
  }
}