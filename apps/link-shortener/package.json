{
  "name": "link-shortener",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "cd ../../packages/prisma-db && npm run generate-db && cd ../../apps/link-shortener && next build",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf .next",
    "check-types": "tsc --noEmit",
    "dev": "next dev --port 3003",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "postinstall": "cd ../../packages/prisma-db && npm run generate-db",
    "start": "next start"
  },
  "dependencies": {
    "@heroicons/react": "2.2.0",
    "next": "16.0.7",
    "prisma-db": "*",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@types/node": "22",
    "@types/react": "19.2.0",
    "@types/react-dom": "19.2.0",
    "eslint": "9.32.0",
    "eslint-config": "*",
    "typescript": "5.9.2",
    "typescript-config": "*"
  }
}