{
  "$schema": "https://turborepo.org/schema.json",
  "extends": [
    "//"
  ],
  "tasks": {
    "build": {
      "outputs": [
        ".next/**",
        "dist/**"
      ],
      "dependsOn": [
        "^build"
      ],
      "env": [
        "NODE_ENV"
      ]
    },
    "dev": {
      "with": [
        "api#dev"
      ],
      "persistent": true,
      "cache": false
    }
  }
}