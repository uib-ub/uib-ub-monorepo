{
  "$schema": "https://turborepo.org/schema.json",
  "globalEnv": [],
  "tasks": {
    "build": {
      "outputs": [
        "dist/**",
        ".next/**",
        ".nuxt/**",
        ".output/**",
        "storybook-static/**"
      ],
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env.production.local",
        ".env.local",
        ".env.production",
        ".env"
      ],
      "env": [
        "DATABASE_URL"
      ]
    },
    "sps-termportal-web#generate": {
      "outputs": [
        "dist/**"
      ]
    },
    "test": {
      "outputs": [
        "coverage/**"
      ],
      "dependsOn": []
    },
    "lint": {},
    "dev": {
      "cache": false,
      "dependsOn": [
        "^build"
      ]
    },
    "transit": {
      "dependsOn": [
        "^transit"
      ]
    },
    "check-types": {
      "dependsOn": [
        "transit"
      ]
    },
    "clean": {
      "dependsOn": [
        "transit"
      ]
    }
  },
  "db:generate": {
    "cache": false
  },
  "db:migrate": {
    "cache": false,
    "persistent": true
  },
  "db:deploy": {
    "cache": false
  }
}