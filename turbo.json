{
  "$schema": "https://turborepo.org/schema.json",
  "globalEnv": [],
  "tasks": {
    "build": {
      "outputs": [
        "dist/**",
        ".next/**",
        "storybook-static/**"
      ],
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env.production.local",
        ".env.local",
        ".env.production",
        ".env"
      ]
    },
    "generate#sps-termportalen-web": {
      "outputs": [
        "dist/**"
      ]
    },
    "test": {
      "outputs": [
        "coverage/**"
      ],
      "dependsOn": []
    },
    "lint": {
      "outputs": [
        "dist/**",
        "build/**"
      ]
    },
    "dev": {
      "cache": false,
      "dependsOn": [
        "^build"
      ]
    }
  }
}