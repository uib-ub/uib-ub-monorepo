{
  "$schema": "https://turborepo.org/schema.json",
  "globalEnv": [],
  "pipeline": {
    "build": {
      "dotEnv": [
        ".env.production.local",
        ".env.local",
        ".env.production",
        ".env"
      ],
      "outputs": [
        "dist/**",
        ".next/**",
        "storybook-static/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "generate#sps-termportalen-web": {
      "outputs": [
        "dist/**"
      ]
    },
    "test": {
      "outputs": [
        "coverage/**"
      ],
      "dependsOn": []
    },
    "lint": {
      "outputs": [
        "dist/**",
        "build/**"
      ]
    },
    "dev": {
      "cache": false,
      "dependsOn": [
        "^build"
      ]
    },
    "dev#chc-exh-nt-web": {
      "cache": false,
      "dependsOn": [
        "^build"
      ],
      "env": [
        "NODE_ENV",
        "NEXT_PUBLIC_STUDIO_URL",
        "NEXT_PUBLIC_STUDIO_URL",
        "NEXT_PUBLIC_SANITY_DATASET",
        "NEXT_PUBLIC_SANITY_PROJECT_ID"
      ]
    },
    "deploy": {
      "cache": false,
      "dependsOn": []
    },
    "clean": {
      "cache": false
    }
  }
}